{# template/users.twig #}
{% extends 'admin/layout.twig' %}
{% block title %}Users{% endblock %}
{% block content %}
    <div class="table-responsive">
        <table class="table table-striped">
            <thead class="thead-inverse">
            <tr>
                <th>Id</th>
                <th>Login</th>
                <th>Email</th>
                <th>Password</th>
                <th>Create date</th>

            </tr>
            </thead>
            <tbody>
            {% if users %}


                {% for user in users %}
                    <tr>
                        <td>{{ user.id }}</td>
                        <td>{{ user.login }}</td>
                        <td>{{ user.email }}</td>
                        <td>{{ user.password }}</td>
                        <td>{{ user.created_at }}</td>
                    </tr>
                {% endfor %}

            {% endif %}


            </tbody>
        </table>
    </div>
{% endblock %}